using System;


namespace ConsoleApp19
{
    class Program
    {
        static void Main(string[] args)
        {
            int x;
            double y;
            Console.WriteLine("Введите значение x");
            x = Convert.ToInt32(Console.ReadLine());
            y = 4 * (Math.Pow(x - 3, 6)) - 7 * (Math.Pow(x - 3, 3)) + 2;
            Console.WriteLine("Значение функции равно " + y);
            Console.ReadKey();

        }
    }
}
