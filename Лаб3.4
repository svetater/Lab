using System;


namespace ConsoleApp18
{
    class Program
    {
        static void Main(string[] args)
        {
            int x;
            double y;
            Console.WriteLine("Введите значение x");
            x = Convert.ToInt32(Console.ReadLine());
            y = 3 *( Math.Pow(x, 6)) - 6 *( Math.Pow(x, 2)) - 7;
            Console.WriteLine("Значение функции равно" + y);
            Console.ReadKey();

        }
    }
}
