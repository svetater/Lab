using System;


namespace ConsoleApp21
{
    class Program
    {
        static void Main(string[] args)
        {
            double a, a1, a2;
            Console.WriteLine("Введите число a");
            a = Convert.ToInt32(Console.ReadLine());
            a1 = Math.Pow(a, 3);
            a2 = Math.Pow(a, 5);
            a = a1 * a2 * a * a * (Math.Pow(a, 5));
            Console.WriteLine("a^15=" + a);
            Console.ReadKey();
        }
    }
}
