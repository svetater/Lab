using System;


namespace ConsoleApp28
{
    class Program
    {
        static void Main(string[] args)
        {
            int a, b, k;
            Console.WriteLine("Длина отрезка a");
            a = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("Длина отрезка b");
            b = Convert.ToInt32(Console.ReadLine());
            k = a / b;
            Console.WriteLine("Количество отрезков b на отрезке a: " + k);
            Console.ReadKey();
        }
    }
}
