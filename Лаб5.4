using System;


namespace ConsoleApp30
{
    class Program
    {
        static void Main(string[] args)
        {
            int a, x1, x2;
            Console.WriteLine("Введите число a");
            a = Convert.ToInt32(Console.ReadLine());
            x1 = a / 10;
            x2 = a % 10;
            a = x2 * 10 + x1;
            Console.WriteLine("Число a после перестановки цифр: " + a);
            Console.ReadKey();
        }
    }
}
